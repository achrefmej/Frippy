<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/ecommerce01"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ecommerce01"
        android:supportsRtl="true"
        android:theme="@style/Theme.Frippy"
        tools:targetApi="31">
        <activity
            android:name="com.example.frippy.ui.RegisterActivity"
            android:exported="false">

        </activity>
        <activity
            android:name="com.example.frippy.ui.LoginActivity"
            android:exported="false"></activity>
        <activity
            android:name="com.example.frippy.ui.OnBoardingActivity"
            android:exported="false" />
        <activity
            android:name="com.example.frippy.ui.Spalsh"
            android:exported="true"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="false"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
    </application>

</manifest>